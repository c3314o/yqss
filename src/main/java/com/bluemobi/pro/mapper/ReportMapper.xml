<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bluemobi.pro.entity.Report">
	
	<insert id="insertReport" parameterType="com.bluemobi.pro.entity.Report">
		INSERT INTO
			report
		(
			`user_id`,
			`sh_id`,
			`content`,
			`create_date`
		)
		VALUES
		(
			#{userId},
			#{shId},
			#{content},
			#{createDate}
		)
	</insert>
</mapper> 